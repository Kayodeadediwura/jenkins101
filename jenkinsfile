pipeline {
    agent any
    environment {
         AWS_SECRET_ACCESS_KEY = credentiails ('AWS_SECRET_ACCESS_KEY')
         AWS_ACCESS_KEY_ID = credentials ('AWS_ACCESS_KEY_ID')
         students = '123456'
     }
    
    stages {
         stage (show environment) {
             steps {
                 sh 'env'
             }
         }
         stage ('second stage') {
             steps {
                 echo 'this is stage one'
                 echo 'it is the second stage'
             }
         }
    }
    
}
