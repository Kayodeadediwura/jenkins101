pipeline {
    agent any
    environment {
         AWS_SECRET_ACCESS_KEY = credentiails ('AWS_SECRET_ACCESS_KEY')
         AWS_ACCESS_KEY_ID = credentials ('AWS_ACCESS_KEY_ID')
     }
    
    stages {
         stage ("initialise terraform") {
             steps {
                 sh ' terraform init '
             }
         }
         stage ("two") {
             steps {
                 echo 'this is step two'
             }
         }
    }
    
}
